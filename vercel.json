{
  "$schema": "https://openapi.vercel.sh/vercel.json",
  "builds": [
    {
      "src": "package.json",
      "use": "@vercel/static-build",
      "config": { "distDir": "dist" }
    }
  ],
  "functions": {
    "api/gpt-lead/index.js": {
      "runtime": "@vercel/node@3.0.0"
    }
  },
  "routes": [
    {
      "src": "/api/gpt-lead",
      "dest": "api/gpt-lead/index.js"
    },
    {
      "src": "/chat",
      "dest": "/index.html"
    },
    {
      "src": "/chat/",
      "dest": "/index.html"
    },
    {
      "src": "/agent-integrations",
      "dest": "/index.html"
    },
    {
      "handle": "filesystem"
    },
    {
      "src": "/(.*)",
      "dest": "/index.html"
    }
  ]
}
